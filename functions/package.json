{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "env-var:get": "firebase functions:config:get",
    "env-var:set": "firebase functions:config:set env=\"$(cat env.json)\"",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "index.js",
  "dependencies": {
    "cloudinary": "^1.23.0",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.19.0",
    "puppeteer": "^13.5.1",
    "query-string": "^6.13.7"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
