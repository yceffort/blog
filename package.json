{
  "version": "0.0.1",
  "name": "yceffort-blog",
  "description": "yceffort-blog",
  "scripts": {
    "dev": "next dev",
    "draft": "ts-node --project ./tsconfig.node.json -e 'import { getAllDraftPosts } from \"./src/utils/Markdown\"; getAllDraftPosts();'",
    "sitemap": "ts-node --project ./tsconfig.node.json ./src/SiteMap.ts",
    "build-nextjs": "next build",
    "build": "npm run sitemap && npm run build-nextjs",
    "start": "next start",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier '**/*.{json,yaml}' --check",
    "prettier:fix": "prettier '**/*.{json,yaml}' --write",
    "algolia": "ts-node --project ./tsconfig.node.json ./src/algolia.ts"
  },
  "repository": "https://github.com/yceffort/yceffort-blog-v2",
  "author": "yceffort <yceffort@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "nextjs",
    "blog"
  ],
  "dependencies": {
