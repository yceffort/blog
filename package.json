    "algoliasearch": "^4.5.1",
    "chrome-aws-lambda": "^5.5.0",
    "cloudinary": "^1.23.0",
    "date-fns": "^2.16.1",
    "front-matter": "^4.0.2",
    "isomorphic-fetch": "^3.0.0",
    "memoizee": "^0.4.14",
    "next": "10.0.1",
    "next-pwa": "^3.1.5",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.22.0",
    "promise-fs": "^2.1.1",
    "puppeteer-core": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "react-is": "^17.0.1",
    "react-tiny-fab": "^4.0.1",
    "rehype-katex": "^4.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-gfm": "^1.0.0",
    "remark-html": "^13.0.1",
